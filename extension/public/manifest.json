{
  "name": "Advanced Timer App",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Advanced Timer App",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "./logo192.png",
    "default_popup": "popup.html",
    "default_title": "advanced timer chrome app"
  },
  "permissions": ["notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
